<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.all.win.dao.UserMapper">

    <insert id="insertUser" parameterType="com.all.win.pojo.User" keyProperty="id" useGeneratedKeys="true">
        INSERT INTO USER (
            name,age,gender,address,phone,mobile_Phone,email,qq,wx,photo
        ) VALUES (
            #{name},#{age}, #{gender}, #{address}, #{phone},
            #{mobilePhone},#{email}, #{qq}, #{wx},#{photo}
        )
    </insert>

    <select id="selectByUserId" parameterType="java.lang.Long" resultType="com.all.win.pojo.User">
        select * from USER where id=#{id}
    </select>


</mapper>
